<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
     
    </tr>
  </thead>

  <tbody>
    <% Category.all.each do |category| %>
      <tr id="category_<%= category.id %>">
      <td><%= link_to category.name, category_path(category.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>